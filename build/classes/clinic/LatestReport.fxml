        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="279.0" minHeight="10.0" prefHeight="82.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="492.0" minHeight="10.0" prefHeight="486.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="125.0" text="اسم المريض :" GridPane.columnIndex="1" />
            <Label alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="125.0" text="أحدث تقرير :" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="patientNameField" editable="false" />
            <TextArea fx:id="latestReport" editable="false" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" />
         </children>
      </GridPane>
