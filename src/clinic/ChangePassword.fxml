<AnchorPane id="AnchorPane" prefHeight="224.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinic.ChangePasswordController">
