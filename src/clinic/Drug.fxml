<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="572.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinic.DrugController">
   <children>
      <TableView fx:id="DrugTable" layoutX="5.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="480.0" prefWidth="331.0">
        <columns>
          <TableColumn fx:id="DrugCol" prefWidth="330.0" text="قائمة الادوية" />
        </columns>
      </TableView>
      <VBox layoutX="367.0" layoutY="189.0" prefHeight="165.0" prefWidth="143.0">
         <children>
            <Button fx:id="AddDrug" mnemonicParsing="false" onAction="#onAdd" prefHeight="33.0" prefWidth="143.0" text="إضافة">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="ChooseDrug" mnemonicParsing="false" onAction="#onSelect" prefHeight="18.0" prefWidth="143.0" text="اختيار">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children></VBox>
   </children>
</AnchorPane>
